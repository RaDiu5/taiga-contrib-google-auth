{
  "name": "taiga-contrib-github-auth",
  "version": "0.0.1",
  "description": "Taiga contrib plugin for google authentication",
  "main": "index.js",
  "scripts": {
    "build-jade": "node_modules/jade/bin/jade.js partials/contrib/google_auth.jade -o dist/",
    "build-coffee": "cat coffee/google_auth.coffee dist/google_auth.html | node_modules/coffee-script/bin/coffee --compile --stdio > dist/google_auth.js",
    "build": "npm run build-jade; npm run build-coffee"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taigaio/taiga-contrib-github-auth"
  },
  "keywords": [
    "google",
    "taiga",
    "authentication",
    "plugin"
  ],
  "author": "HanWool Lee <kudnya@gmail.com>",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/taigaio/taiga-contrib-github-auth/issues"
  },
  "homepage": "https://github.com/taigaio/taiga-contrib-github-auth",
  "dependencies": {
    "coffee-script": "^1.8.0",
    "jade": "^1.9.1"
  },
  "devDependencies": {
    "gulp": "^3.9.0",
    "gulp-angular-templatecache": "^1.8.0",
    "gulp-cached": "^1.1.0",
    "gulp-coffee": "^2.3.1",
    "gulp-concat": "^2.6.0",
    "gulp-jade": "^1.1.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-remember": "^0.3.0",
    "gulp-uglify": "^1.5.2",
    "merge-stream": "^1.0.0"
  }
}
